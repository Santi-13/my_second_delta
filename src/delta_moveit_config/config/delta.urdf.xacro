<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="delta">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import delta urdf file -->
    <xacro:include filename="$(find my_third_delta)/urdf/delta.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="delta.ros2_control.xacro" />


    <xacro:delta_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
